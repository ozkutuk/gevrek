{
  fibonacci = \n -> case n of {
    0 -> 1;
    1 -> 1;
    -- TODO(ozkutuk): modify parser to allow single quotes in identifiers
    -- TODO(ozkutuk): modify parser to allow underscores in identifiers,
    -- currently tokenized as wildcard
    _ -> add (fibonacci (sub n 1)) (fibonacci (sub n 2))
  };

  fibonacciBetter =
    let
      go = \k -> \a -> \b -> case k of {
        0 -> b;
        _ -> go (sub k 1) b (add a b)
      }
    in
      \n -> go n 0 1

}
